<h1>New page</h1>

<% form_for(@page) do |f| %>
  <%= f.error_messages %>

  <p>
		<%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
		<%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
		<%= f.label :permalink %><br />
    <%= f.text_field :permalink %>
  </p>
  <p>
		<%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pages_path %>