<h1>Editing attendance</h1>

<% form_for(@attendance) do |f| %>
  <%= f.error_messages %>

  <p>
		<%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
		<%= f.label :show_id %><br />
    <%= f.text_field :show_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @attendance %> |
<%= link_to 'Back', attendances_path %>